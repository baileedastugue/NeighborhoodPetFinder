<nav class="navbar navbar-light bg-light py-3">
     <a class="navbar-brand">Your Friendly Neighborhood Pet Adoption</a>
     <form class="form-inline">
          <button class="btn btn-outline-success m-2 my-sm-0" type="button" id="searchPageBtn">Search Page</button>
          <button class="btn btn-outline-success m-2 my-sm-0" type="button" id="logOutBtn">Log Out</button>
     </form>
</nav>

<div class="container-fluid">
     <div class="row my-3">
          <div class="col-md-12 text-center">
               <h2>Welcome --Place Holder For User Name--</h2>
          </div>
     </div>
</div>

<div class="container-fluid">
     <h3>Pets Found:</h3>
     <ul id="chosenPetList" class="list-group">
          {{#each ChosenPets}}
          <li data-id="{{dataValues.id}}" class="list-group-item">
               <a href="{{dataValues.url}}" target="_blank">{{dataValues.petName}}</a>
               <button data-id="{{dataValues.id}}" class="btn btn-danger float-right delete">Delete</button>
          </li>
          {{/each}}
     </ul>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/indexApiCalls.js"></script>
<script src="/js/indexOnClickHandlers.js"></script>
<script src="/js/index.js"></script>